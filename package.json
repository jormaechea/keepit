{
  "name": "keepit",
  "version": "1.0.0",
  "description": "Save fabulous npm packages to use them later",
  "main": "lib/index.js",
  "scripts": {
    "test": "export TEST_ENV=true; mocha --exit -R nyan --recursive tests/",
    "test-ci": "nyc --reporter=html --reporter=text mocha --recursive tests/",
    "watch-test": "export TEST_ENV=true; mocha --exit -R nyan -w --recursive tests/",
    "coverage": "nyc npm test",
    "lint": "eslint lib/ tests/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jormaechea/keepit.git"
  },
  "bin": {
    "keepit": "lib/cli.js"
  },
  "author": "Janis",
  "license": "ISC",
  "homepage": "https://github.com/jormaechea/keepit.git#readme",
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "mocha": "^8.1.1",
    "nyc": "^15.1.0"
  },
  "files": [
    "lib/"
  ],
  "directories": {
    "test": "tests"
  },
  "dependencies": {
    "cli-table3": "^0.6.0",
    "fuse.js": "^6.4.1",
    "node-fetch": "^2.6.0",
    "preferences": "^2.0.2",
    "terminal-link": "^2.1.1",
    "yargs": "^15.4.1"
  }
}
